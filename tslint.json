{
    "extends": "tslint:latest",
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "prefer-const": [
            false
        ],
        "semicolon": [
            true,
            "never"
        ],
        "object-literal-sort-keys": false,
        "curly": false,
        "no-namespace": false,
        "member-access": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "no-var-requires": false,
        "arrow-parens": false,
        "no-unused-variable": true,
        "no-string-literal": false,
        "interface-name": false
    }
}
