{
    "extends": "tslint:latest",
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "prefer-const": [
            false
        ],
        "semicolon": [
            true,
            "never"
        ],
        "object-literal-sort-keys": false,
        "curly": false,
        "no-namespace": false,
        "member-access": [
            false
        ]
    }
}
